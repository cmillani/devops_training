default_platform(:ios)

platform :ios do
    desc "Build App"
    lane :build do
      setup_jenkins
      build_number = increment_build_number
      commit_version_bump(message:"[ci skip] Version Bump to #{build_number}")
      # build_app(scheme: "devops_training")
      push_to_git_remote(
        remote: "origin",
        local_branch: "HEAD",
        remote_branch: "master"
      )
    end
  end
